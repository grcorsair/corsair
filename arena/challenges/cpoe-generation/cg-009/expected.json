{
  "schema": {
    "type": "CorsairCPOE",
    "requiredFields": ["scope", "provenance", "summary", "processProvenance"],
    "provenance": {
      "source": "tool",
      "sourceIdentity": "Prowler",
      "requiredFields": ["source", "sourceIdentity"]
    },
    "processProvenance": {
      "requiredFields": ["chainDigest", "receiptCount", "chainVerified", "format"],
      "format": "in-toto/v1+cose-sign1",
      "chainVerified": true
    },
    "summary": {
      "controlsTested": 15,
      "controlsPassed": 10,
      "controlsFailed": 5,
      "scoreRange": [63, 70]
    }
  },
  "jwtHeaders": {
    "alg": "EdDSA",
    "typ": "vc+jwt"
  },
  "vcContext": ["https://www.w3.org/ns/credentials/v2"],
  "vcType": ["VerifiableCredential", "CorsairCPOE"]
}
